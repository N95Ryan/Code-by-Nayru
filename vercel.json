{
  "buildCommand": "npm run build",
  "outputDirectory": "dist",
  "redirects": [
    {
      "source": "/",
      "destination": "/fr/",
      "permanent": true
    },
    {
      "source": "/(?!fr/|en/).*",
      "destination": "/fr/$1",
      "permanent": true
    }
  ],
  "trailingSlash": true,
  "cleanUrls": false
} 